    <div class="container" style="padding-top: 5%;">
        <div class="row">
            <div class="col-2"></div>
            <div class="col-8" style="border : 1px black solid; background-color:rgb(158, 170, 240);">
                <div class="row mb-3 mt-4">
                    <div class="col-12" style="text-align : center;">
                        <h2>  <td><%= result.firstname %>'s  </td>Registered Details</h2>
                    </div>
                </div>
                <div class="row mb-3">
                    <div class="col-12">
                        <table class="table table-bordered">
                            <tr>
                                <th>First Name</th>
                                <td><%= result.firstname %></td>
                            </tr>
                            <tr>
                                <th>Last Name</th>
                                <td><%= result.lastname %></td>
                            </tr>
                        
                            <tr>
                                <th>email</th>
                                <td><%= result.email %></td>
                            </tr>

                         
                            <tr>
                                <th>ph</th>
                                <td><%= result.ph %></td>
                            </tr>
                            <tr>
                                <th>country</th>
                                <td><%= result.country %></td>
                            </tr>

                            <tr>
                                <th>address</th>
                                <td><%= result.employee %></td>
                            </tr>

                            <tr>
                              <th> Resume  (IPFS URL)</th>
                              <td>
                                  <% if( result._url){ %>
                                  <a href="https://ipfs.io/ipfs/<%= result._url %>"><%= result._url %></a>
                                  <% } else { %>
                                  <h4>Resume  not found !</h4>
                                  <form method="POST" enctype="multipart/form-data" action="ipfs">
                                      <input type="hidden" value="<%= empid %>" name="empidurl" />
                                      <input type="file" name="uploadFile" required />
                                      <br /><br />
                                      <input type="submit" class="btn" value="Upload resume" />
                                  </form>
                                  <% } %>
                              </td>
                          </tr>


                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


